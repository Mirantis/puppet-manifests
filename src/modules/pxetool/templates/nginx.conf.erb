server {
    server_name <%= @fqdn %>;
    listen 80;

    access_log /var/log/nginx/pxetool.access.log proxy;
    error_log /var/log/nginx/pxetool.error.log;

    location / {
        include uwsgi_params;
        uwsgi_pass 127.0.0.1:7931;
    }

    location /static/ {
        root /usr/share/pxetool/webapp/pxetool;
    }
}
