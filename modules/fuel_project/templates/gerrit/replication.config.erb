
[remote "<%= @title %>"]
	url = <%= @user %>@<%= @host %>:<%= @path %>${name}.git
<% if @replication_delay != nil -%>
	replicationDelay = <%= @replication_delay %>
<% end -%>
<% if @threads != nil -%>
	threads = <%= @threads %>
<% end -%>
<% if @auth_group != nil -%>
	authGroup = <%= @auth_group %>
<% end -%>
<% if @replicate_permissions != nil -%>
	replicatePermissions = <%= @replicate_permissions %>
<% end -%>
<% if @mirror != nil -%>
	mirror = <%= @mirror %>
<% end -%>
